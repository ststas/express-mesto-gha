{
    "env": {
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "no-useless-escape": []
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx", ".ts", ".tsx"]
        }
      }

    }
}
